	EDITTEXT		IDC_VEGA, 80, 220, 60, 10, ES_READONLY
	LTEXT			"Theta", IDC_STATIC, 10, 240, 60, 10
	EDITTEXT		IDC_THETA, 80, 240, 60, 10, ES_READONLY
	LTEXT			"Rho", IDC_STATIC, 10, 260, 60, 10
	EDITTEXT		IDC_RHO, 80, 260, 60, 10, ES_READONLY
END


